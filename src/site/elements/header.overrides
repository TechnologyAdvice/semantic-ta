/*******************************
         Site Overrides
*******************************/
h1.plus.ui.header {
  font-size: 3.55rem;
}
h1.minus.ui.header {
  font-size: 2.25rem;
}
/* Mobily Only */
@media only screen and (max-width : @largestMobileScreen) {
  h1.plus.ui.header {
    font-size: 2.5rem;
  }
  h1.ui.header,
  h1.minus.ui.header {
    font-size: 1.45rem;
  }
}
